{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*).wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        }
      ]
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "composeApp/build/dist/wasmJs/productionExecutable"
      }
    }
  ],
  "installCommand": "yum install -y java-21-amazon-corretto-devel",
  "buildCommand": "sh build.sh",
  "outputDirectory": "composeApp/build/dist/wasmJs/productionExecutable"
}
